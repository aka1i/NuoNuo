<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SmartLoadingView">
        <attr name="textStr" format="string" />
        <attr name="errorStr" format="string" />
        <attr name="cannotclickBg" format="color" />
        <attr name="errorBg" format="color" />
        <attr name="normalBg" format="color" />
        <attr name="cornerRaius" format="dimension" />
        <attr name="textColor" format="color" />
        <attr name="textSize" format="dimension" />
        <attr name="scrollSpeed" format="integer" />
    </declare-styleable>

    <declare-styleable name="WaveView">
        <attr name="A" format="integer" />
        <attr name="omiga" format="float" />
        <attr name="K" format="integer" />
        <attr name="bg_color" format="color"/>
        <attr name="fi" format="float"/>
        <attr name="change_fi" format="float"/>
    </declare-styleable>

    <declare-styleable name="WaveButton">
        <attr name="waveSrc" format="reference" />
        <attr name="waveColor" format="color" />
        <attr name="progressColor" format="color" />
        <attr name="waveMaxRadius" format="dimension" />
        <attr name="waveMinRadius" format="dimension" />
    </declare-styleable>
</resources>
